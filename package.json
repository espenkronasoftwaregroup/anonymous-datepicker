{
  "name": "simple-calendar",
  "version": "0.1.0",
  "private": true,
  "main": "src/SimpleCalendar.tsx",
  "dependencies": {
    "luxon": "3.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "4.7.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "ladle serve"
  },
  "eslintConfig": {
    "extends": [

      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/luxon": "3.0.0",
    "@types/node": "16.11.49",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@ladle/react": "^2.4.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.74.0"
  }
